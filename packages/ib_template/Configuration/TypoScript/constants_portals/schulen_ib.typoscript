        #misc.mapViewImg = fileadmin/user_upload/navigation/xxx.jpg

        #Google Analytics ID
        analytics {
            #account_id =
            #enable_tracking = 0
            matomo_enabled = 0
            matomo_siteid = 50
            matomo_tagmanager_enabled = 1
            matomo_containerid = te3LgFU5
        }

        cookiebot {
            enabled = 1
            cbid = ae8bb82d-c0c6-49ac-a6f4-9fe8473b27db
        }

        #Text und Verlinkung des Bubbles im Header
        main_bubble {
            hide = 0
