        #data privacy site ID
        dataPrivacySiteID = 5025

        readspeaker.show = 1

        #Navigation / Mainnavigation
        mainnav_start_id = 5030

        #Start-ID für Sitemap / zB. Mainnavigation
        misc.sitemapStartId = 5030

        #Präfix Titel
        firstMenuTitle = Schulen IB

        #News Details - Seite
        tt_news.single_pid = 5035

        #Konfiguration Suchmaske / Standorte+Angebote ausblenden = 1
        misc.showOnlyTYPO3SearchResults = 0

        #Suche - Ergebnisseite
