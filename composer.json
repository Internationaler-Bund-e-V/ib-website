{
    "name": "typo3/cms-base-distribution",
    "description": "TYPO3 CMS Base Distribution",
    "license": "GPL-2.0+",
    "require": {
        "fluidtypo3/vhs": "^7.0",
        "netresearch/rte-ckeditor-image": "^11.0.6",
        "apache-solr-for-typo3/solr": "^11.5.0",
        "evoweb/recaptcha": "^11.0",
        "georgringer/news": "^11.4",
        "helhum/typo3-console": "^7.1",
        "lochmueller/staticfilecache": "^13.1",
        "mask/mask": "^8.3",
        "typo3/cms-backend": "^11.5",
        "typo3/cms-belog": "^11.5",
        "typo3/cms-beuser": "^11.5",
        "typo3/cms-core": "^11.5",
        "typo3/cms-extbase": "^11.5",
        "typo3/cms-extensionmanager": "^11.5",
        "typo3/cms-felogin": "^11.5",
        "typo3/cms-filelist": "^11.5",
        "typo3/cms-filemetadata": "^11.5",
        "typo3/cms-fluid": "^11.5",
        "typo3/cms-fluid-styled-content": "^11.5",
        "typo3/cms-form": "^11.5",
        "typo3/cms-frontend": "^11.5",
        "typo3/cms-info": "^11.5",
        "typo3/cms-install": "^11.5",
        "typo3/cms-lowlevel": "^11.5",
        "typo3/cms-recordlist": "^11.5",
        "typo3/cms-recycler": "^11.5",
        "typo3/cms-redirects": "^11.5",
        "typo3/cms-rte-ckeditor": "^11.5",
        "typo3/cms-seo": "^11.5",
        "typo3/cms-setup": "^11.5",
        "typo3/cms-tstemplate": "^11.5",
        "typo3/cms-viewpage": "^11.5",
        "typo3/cms-workspaces": "^11.5",
        "typo3/cms-opendocs": "^11.5",
        "typo3/cms-adminpanel": "^11.5",
        "apache-solr-for-typo3/tika": "^11.0",
        "ib/ib-cmt": "@dev",
        "rms/ib-contact-person": "@dev",
        "rms/ib-dataprivacy": "@dev",
        "rms/ib-formbuilder": "@dev",
        "rms/ib-galerie": "@dev",
        "rms/ib-page-extension": "@dev",
        "rms/ib-srb": "@dev",
        "rms/ib-template": "@dev",
        "rms/ib-content": "@dev",
        "rms/ib-jobs": "@dev",
        "rms/ib-search": "@dev",
        "rms/bv_bbe": "@dev",
        "apache-solr-for-typo3/solrfal": "@dev",
        "apache-solr-for-typo3/solrconsole": "@dev",
        "georgringer/numbered-pagination": "^1.0",
        "spooner-web/be_secure_pw": "^10.1",
        "beechit/fal-securedownload": "^5.0",
        "studiomitte/recordlist-thumbnail": "^1.0",
        "studiomitte/friendlycaptcha": "^0.1",
        "dirkpersky/typo3-rte-ckeditor-fontawesome": "^11.5.6",
        "ssch/typo3-encore": "^5.0",
        "symfony/webpack-encore-bundle": "^2.2"
    },
    "require-dev": {
        "ssch/typo3-rector": "^2.5",
        "cakephp/cakephp-codesniffer": "^5.0",
        "phpstan/phpstan": "^1.10"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "public"
        }
    },
    "autoload": {
        "psr-4": {
            "Rms\\Ibcontent\\": "packagesibcontent/Classes/",
            "Rms\\Ibjobs\\": "packagesibjobs/Classes/",
            "Rms\\Ibsearch\\": "packagesibsearch/Classes/",
            "Rms\\IbFormbuilder\\": "packagesib_formbuilder/Classes/",
            "Rms\\IbGalerie\\": "packagesib_galerie/Classes/",
            "Rms\\IbContactPerson\\": "packagesib_contact_person/Classes/",
            "Rms\\IbDataprivacy\\": "packagesib_dataprivacy/Classes/",
            "IB\\IbCmt\\": "packagesib_cmt/Classes/",
            "ApacheSolrForTypo3\\Solrfal\\": "packagessolrfal/Classes/",
            "SFC\\Staticfilecache\\": "public/typo3conf/ext/staticfilecache/Classes/",
            "ApacheSolrForTypo3\\Tika\\": "public/typo3conf/ext/tika/Classes/",
            "ApacheSolrForTypo3\\Solr\\": "public/typo3conf/ext/solr/Classes/"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*",
            "options": {
                "symlink": true
            },
            "only": [
                "ib/ib-cmt",
                "rms/ib-contact-person",
                "rms/ib-dataprivacy",
                "rms/ib-formbuilder",
                "rms/ib-galerie",
                "rms/ib-page-extension",
                "rms/ib-srb",
                "rms/ib-template",
                "rms/ib-content",
                "rms/ib-jobs",
                "rms/ib-search",
                "rms/bv_bbe",
                "apache-solr-for-typo3/solrfal",
                "apache-solr-for-typo3/solrconsole"
            ]
        },
        {
            "type": "composer",
            "url": "https://composer.typo3.org/"
        }
    ],
    "scripts": {
        "typo3-cms-scripts": [
            "typo3cms install:fixfolderstructure"
        ],
        "post-autoload-dump": [
            "@typo3-cms-scripts"
        ]
    }
}
